#define BCONVOLCHARGECMI_12    ((int32)1*BATCAPACITY)               ////
#define BCONVOLCHARGECMV_12    ((int32)23*BATCELLS)                 ////
#define BCONVOLCHARGEENDV_12   ((int32)237*BATCELLS/10)             ////无
#define BCONVOLCHARGEENDCNT_12 ((int32)CHARGR_TIME/1000)               ////无
//浮充阶段充电信息：电流电压指令，跳转时间
#define FLOATCHARGECMI_12      ((int32)BATCAPACITY)                 ////指令电流0.01C
#define FLOATCHARGECMV_12      ((int32)247*BATCELLS/10)             ////指令电压电池总电压6.9*4V
#define FLOATCHARGEENDCNT_12   ((int32)6*CHARGR_TIME)               ////浮充结束时间4H
*/

//曲线L13
//预充阶段充电信息：电流电压指令，跳转电压和时间
#define PRECHARGECMI_13        ((int32)3*BATCAPACITY)               //指令电流0.03C
#define PRECHARGECMV_13        ((int32)21*BATCELLS)                 //指令电压电池总电压
#define PRECHARGEENDV_13       ((int32)200*BATCELLS/10)             //预充结束电池总电压
#define PRECHARGEENDCNT_13     ((int32)6*CHARGR_TIME)               //预充结束时间2H
//恒流充电一阶段充电信息：电流电压指令，跳转电压和跳转时间在程序里计算
#define CONCURCHARGECMI_13     ((int32)4*BATCAPACITY)              //指令电流0.12C(标准0.15-0.35C)
#define CONCURCHARGECMV_13     ((int32)24.5*BATCELLS)                 //指令电压电池总电压
#define CONCURCHARGEENDI_13    ((int32)4*BATCAPACITY)
#define CONCURCHARGEENDV_13    ((int32)235*BATCELLS/10)             //恒流结束电池总电压28.2V
#define CONCURCHARGEENDCNT_13    ((int32)8*CHARGR_TIME)               ////3小时
//恒流充电二阶段充电信息：电流电压指令，跳转电压和时间
#define SCONCURCHARGECMI_13    ((int32)4*BATCAPACITY)              //指令电流0.12C
#define SCONCURCHARGECMV_13    ((int32)245*BATCELLS/10)                 //指令电压电池总电压
#define SCONCURCHARGEENDV_13   ((int32)240*BATCELLS/10)             //恒流结束电池总电压29.4V
#define SCONCURCHARGEENDCNT_13 ((int32)8*CHARGR_TIME)               //恒流结束时间6H
//恒压充电阶段充电信息：电流电压指令，跳转电流和时间
#define CONVOLCHARGECMI_13     ((int32)4*BATCAPACITY)              //指令电流0.12C
#define CONVOLCHARGECMV_13     ((int32)240*BATCELLS/10)             //指令电压电池总电压29.4V
#define CONVOLCHARGEENDI_13    ((int32)1*BATCAPACITY)               //恒压结束电池总电流
#define CONVOLCHARGEENDCNT_13  ((int32)6*CHARGR_TIME)               //恒压结束时间3H
//涓流充电阶段充电信息：电流电压指令，跳转时间
#define BCONVOLCHARGECMI_13    ((int32)1.5*BATCAPACITY)               //
#define BCONVOLCHARGECMV_13    ((int32)260*BATCELLS/10)             //
#define BCONVOLCHARGEENDV_13   ((int32)258*BATCELLS/10)             //
#define BCONVOLCHARGEENDCNT_13 ((int32)10*CHARGR_TIME)               //
//浮充阶段充电信息：电流电压指令，跳转时间
#define FLOATCHARGECMI_13      ((int32)1.5*BATCAPACITY)           //指令电流0.015C
#define FLOATCHARGECMV_13      ((int32)259*BATCELLS/10)             //指令电压电池总电压
#define FLOATCHARGEENDCNT_13   ((int32)6*CHARGR_TIME)               //预充结束时间6H
