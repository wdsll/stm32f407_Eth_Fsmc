#define CONVOLCHARGEENDI_17    ((int32)1*BATCAPACITY)               //恒压结束电池总电流
#define CONVOLCHARGEENDCNT_17  ((int32)6*CHARGR_TIME)               //恒压结束时间3H
//涓流充电阶段充电信息：电流电压指令，跳转时间
#define BCONVOLCHARGECMI_17    ((int32)1.5*BATCAPACITY)               //
#define BCONVOLCHARGECMV_17    ((int32)260*BATCELLS/10)             //
#define BCONVOLCHARGEENDV_17   ((int32)258*BATCELLS/10)             //
#define BCONVOLCHARGEENDCNT_17 ((int32)10*CHARGR_TIME)               //
//浮充阶段充电信息：电流电压指令，跳转时间
#define FLOATCHARGECMI_17      ((int32)1.5*BATCAPACITY)           //指令电流0.015C
#define FLOATCHARGECMV_17      ((int32)259*BATCELLS/10)             //指令电压电池总电压
#define FLOATCHARGEENDCNT_17   ((int32)CHARGR_TIME)               //预充结束时间0.5H
//EQ充电阶段信息：电流时间指令
#define EQCurrCMD_17  ((int32)7*BATCAPACITY_17)                   //指令电流0.07C
#define EQVoltCMD_17  ((int32)272*BATCELLS/10)                   //指令电压电池总电压
#define EQTimeCMD_17   ((int32)12*CHARGR_TIME)                    //预充结束时间1H

