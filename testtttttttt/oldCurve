#ifndef LLCCURVE_H_
#define LLCCURVE_H_

#include "DSP28x_Project.h"    // Device Headerfile and Examples Include File

/*******************************************************************************
**                               宏定义
*******************************************************************************/
#define BATCELLS            12                    //电池节数、电池负载总的电池串数
#define BATCAPACITY         251                   //电池容量、标称电池负载容量
#define BATCAPACITY_1         225        //电池容量、标称电池负载容量b02:T105(225Ah)(Trojan)
#define BATCAPACITY_2         240        //电池容量、标称电池负载容量b07:T125(240Ah)(Trojan)
#define BATCAPACITY_3         260        //电池容量、标称电池负载容量b10:T145(260Ah)(Trojan)
#define BATCAPACITY_4         300        //电池容量、标称电池负载容量b11:T1275(300Ah)(Trojan)
#define BATCAPACITY_5         200        //电池容量、标称电池负载容量100:DT1275(200Ah)(Leoch)
#define BATCAPACITY_6         100        //电池容量、标称电池负载容量101:DT1275(100Ah)(Leoch)
#define BATCAPACITY_7         160        //电池容量、标称电池负载容量102:DT126(160Ah)(Leoch)
#define BATCAPACITY_8         155        //电池容量、标称电池负载容量103:DT103(155Ah)(Leoch)
#define BATCAPACITY_9         115        //电池容量、标称电池负载容量104:27TMH(115Ah)(Trojan)
#define BATCAPACITY_10         155       //电池容量、标称电池负载容量b94:Hoppecke
#define BATCAPACITY_11         250       //电池容量、标称电池负载容量b38:Hoppecke
#define BATCAPACITY_12         155       //电池容量、标称电池负载容量b22:Hoppecke
#define BATCAPACITY_13         250       //电池容量、标称电池负载容量b81:FULLRIVER
#define BATCAPACITY_14         155       //电池容量、标称电池负载容量b90:FULLRIVER
#define BATCAPACITY_15         250       //电池容量、标称电池负载容量b91:FULLRIVER
#define BATCAPACITY_16         155       //电池容量、标称电池负载容量b25FULLRIVER
#define BATCAPACITY_17         250       //电池容量、标称电池负载容量b92:FULLRIVER
#define CHARGR_TIME         1800000      //充电时间单位毫秒半小时
#define Ress_Line           34*2>>10              ///  1.5米5.1毫欧，正负是3.4mΩ/m///1024==1000
#define Line_Lengths        15/100             ///  15//1.5m  输出线单边距离长度
#define Line_Loss_Vol       ((int32)((IOUT_DATA * K_IOUT_A + K_IOUT_B) >> K_EXP)*2/1000)
// =============================================================================
// 当前工程文件对应的电池充电曲线
// =============================================================================
#define CHGSTAT_PRECHARGE       0   // 预充状态
#define CHGSTAT_CONCURCHARGE    1   // 恒流充电一状态
#define CHGSTAT_SCONCURCHARGE   2   // 恒流充电二状态
#define CHGSTAT_CONVOLCHARGE    3   // 恒压充电一状态
#define CHGSTAT_BCONVOLCHARGE   4   // 涓流充电状态
#define CHGSTAT_FLOATCHARGE     5   // 浮充状态
#define CHGSTAT_EQMODECHARGE    6   // EQ模式充电
#define CHGSTAT_PAUSEMODE       7   // 暂停充电状态

//曲线b02:Trojan T105 225Ah

//预充阶段充电信息：电流电压指令，跳转电压和时间
#define PRECHARGECMI_1        ((int32)2*BATCAPACITY_1)               //指令电流0.03C
#define PRECHARGECMV_1        ((int32)21*BATCELLS)                 //指令电压电池总电压
#define PRECHARGEENDV_1       ((int32)190*BATCELLS/10)             //预充结束电池总电压
#define PRECHARGEENDCNT_1     ((int32)2*CHARGR_TIME)               //预充结束时间2H
//恒流充电一阶段充电信息：电流电压指令，跳转电压和跳转时间在程序里计算
#define CONCURCHARGECMI_1     ((int32)12*BATCAPACITY_1)              //指令电流0.12C
#define CONCURCHARGECMV_1     ((int32)224*BATCELLS/10)              //指令电压电池总电压//234
#define CONCURCHARGEENDI_1    ((int32)14*BATCAPACITY_1)
#define CONCURCHARGEENDV_1    ((int32)220*BATCELLS/10)             //恒流结束电池总电压//230
#define CONCURCHARGEENDCNT_1    ((int32)10*CHARGR_TIME)            ////5小时
//恒流充电二阶段充电信息：电流电压指令，跳转电压和时间
#define SCONCURCHARGECMI_1    ((int32)12*BATCAPACITY_1)              //指令电流0.13C
#define SCONCURCHARGECMV_1    ((int32)245*BATCELLS/10)             //指令电压电池总电压
#define SCONCURCHARGEENDV_1   ((int32)243*BATCELLS/10)             //恒流结束电池总电压
#define SCONCURCHARGEENDCNT_1 ((int32)16*CHARGR_TIME)               //恒流结束时间8H
//恒压充电阶段充电信息：电流电压指令，跳转电流和时间
#define CONVOLCHARGECMI_1     ((int32)12*BATCAPACITY_1)              //指令电流0.12C
#define CONVOLCHARGECMV_1     ((int32)245*BATCELLS/10)             //指令电压电池总电压
#define CONVOLCHARGEENDI_1    ((int32)2*BATCAPACITY_1)               //恒压结束电池总电流
#define CONVOLCHARGEENDCNT_1  ((int32)6*CHARGR_TIME)               //恒压结束时间3H
//涓流充电阶段充电信息：电流电压指令，跳转时间
#define BCONVOLCHARGECMI_1    ((int32)2*BATCAPACITY_1)               //指令电流0.02C
#define BCONVOLCHARGECMV_1    ((int32)270*BATCELLS/10)             //指令电压电池总电压
#define BCONVOLCHARGEENDV_1   ((int32)268*BATCELLS/10)             //恒流结束电池总电压
#define BCONVOLCHARGEENDCNT_1 ((int32)6*CHARGR_TIME)               //恒压结束时间3H
//浮充阶段充电信息：电流电压指令，跳转时间
#define FLOATCHARGECMI_1      ((int32)2*BATCAPACITY_1)                 //指令电流0.01C
#define FLOATCHARGECMV_1      ((int32)223*BATCELLS/10)             //指令电压电池总电压
#define FLOATCHARGEENDCNT_1   ((int32)2*CHARGR_TIME)               //预充结束时间1H
//EQ充电阶段信息：电流时间指令
#define EQCurrCMD_1  ((int32)7*BATCAPACITY_1)                   //指令电流0.07C
#define EQVoltCMD_1  ((int32)272*BATCELLS/10)                   //指令电压电池总电压
#define EQTimeCMD_1   ((int32)12*CHARGR_TIME)                    //预充结束时间1H
